<h2>Sign Up</h2>
<form action="<%= users_url %>" method="post">
  <%= auth_token %>
  <label for="username">Username</label>
  <input type="text"
         id="username"
         name="user[username]">
  <label for="password">Password</label>
  <input type="password"
          id="password"
          name="user[password]">
  <input type="submit" value="Sign Up">
</form>
